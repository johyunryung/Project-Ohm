<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/BaseLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginHorizontal="30.67dp"
    android:layout_marginVertical="33.33dp"
    android:paddingHorizontal="0dp"
    tools:context=".WaitingRoomActivity">

    <ImageButton
        android:id="@+id/back_btn_nickname_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_toback" />

    <Button
        android:id="@+id/next_btn_nickname_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:minWidth="0dp"
        android:minHeight="0dp"
        android:text="건너뛰기"
        android:textColor="#000000"
        android:textSize="13.33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/h3_nickname_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="104.33dp"
        android:gravity="center"
        android:text="여러분을 더 잘 알아볼 수 있게, \n프로필 사진을 등록해주세요!"
        android:textColor="#000000"
        android:textSize="13.33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>