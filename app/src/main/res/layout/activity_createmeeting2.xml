<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".createmeeting_2">

    <Button
        android:id="@+id/back_button2"
        android:layout_width="18.617dp"
        android:layout_height="14.240dp"
        android:layout_marginStart="30.667dp"
        android:layout_marginTop="33.333dp"
        android:layout_marginEnd="310.733dp"
        android:layout_marginBottom="592.433dp"
        android:background="@drawable/ic_toback"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/next_button2"
        android:layout_width="24.667dp"
        android:layout_height="19.333dp"
        android:layout_marginStart="255.400dp"
        android:layout_marginTop="30.667dp"
        android:layout_marginEnd="30.667dp"
        android:layout_marginBottom="592.433dp"
        android:background="#00ff0000"
        android:text="다음"
        android:textColor="#4D000000"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/back_button2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="148dp"
        android:layout_height="19.333dp"
        android:layout_marginStart="106dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="106dp"
        android:text="회의 주제를 입력해 주세요."
        android:textColor="#CC000000"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/meeting_topic"
        android:layout_width="260dp"
        android:layout_height="0dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="50dp"
        android:maxLength="30"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_number"
        android:layout_width="29dp"
        android:layout_height="16dp"
        android:layout_marginStart="290dp"
        android:layout_marginTop="13.333dp"
        android:layout_marginEnd="49.333dp"
        android:layout_marginBottom="356dp"
        android:text="0/30"
        android:textColor="#CC000000"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/meeting_topic"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>